{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "tabs",
    "scopeName": "source.tabs",
    "patterns": [
        {
            "include": "#comment"
        },
        {
            "include": "#constant"
        },
        {
            "include": "#entity"
        },
        {
            "include": "#invalid"
        },
        {
            "include": "#keyword"
        },
        {
            "include": "#meta"
        },
        {
            "include": "#operator"
        },
        {
            "include": "#punctuation"
        },
        {
            "include": "#storage"
        },
        {
            "include": "#string"
        },
        {
            "include": "#support"
        },
        {
            "include": "#type"
        },
        {
            "include": "#variable"
        }
    ],
    "repository": {
        "comment": {
            "patterns": []
        },
        "constant": {
            "patterns": [
                {
                    "comment": "Notes",
                    "match": "(\\d)",
                    "name": "constant.numeric.fret.tabs"
                }
            ]
        },
        "entity": {
            "patterns": []
        },
        "invalid": {
            "patterns": []
        },
        "keyword": {
            "patterns": []
        },
        "meta": {
            "patterns": []
        },
        "operator": {
            "patterns": []
        },
        "punctuation": {
            "patterns": [
                {
                    "match": "(-)",
                    "name": "punctuation.other.dash.tabs"
                },
                {
                    "match": "([\\[\\]])",
                    "name": "punctuation.definition.section.tabs"
                },
                {
                    "match": "(x)",
                    "name": "punctuation.other.dead-note.tabs"
                }
            ]
        },
        "storage": {
            "patterns": [
                {
                    "match": "^(\\[)\\s+(\\w+)\\s+(\\]$)",
                    "captures": {
                        "1": {
                            "name": "punctuation.definition.section.tabs"
                        },
                        "2": {
                            "name": "variable.other.section.tabs"
                        }
                    }
                }
            ]
        },
        "string": {
            "patterns": [
                {
                    "name": "string.quoted.single.tabs",
                    "begin": "'",
                    "end": "'"
                },
                {
                    "name": "string.quoted.double.tabs",
                    "begin": "\"",
                    "end": "\""
                }
            ]
        },
        "support": {
            "patterns": []
        },
        "type": {
            "patterns": []
        },
        "variable": {
            "patterns": []
        }
    }
}