{
    "name": "vscode-tabs-syntax",
    "displayName": "vscode-tabs-syntax",
    "description": "Syntax highlighting for guitar tablature.",
    "version": "0.0.1",
    "publisher": "tcd",
    "repository": {
        "type": "git",
        "url": "https://github.com/dunstontc/vscode-tabs-syntax.git"
    },
    "bugs": {
        "url": "https://github.com/dunstontc/vscode-tabs-syntax/issues"
    },
    "scripts": {
        "start": "node ./src/main.js"
    },
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tabs",
                "aliases": [
                    "tabs"
                ],
                "extensions": [
                    ".tabs"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tabs",
                "scopeName": "source.tabs",
                "path": "./syntaxes/tabs.tmLanguage.json"
            }
        ]
    },
    "galleryBanner": {
        "color": "#1e1e1e",
        "theme": "dark"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.198",
        "coolkit-js": "^1.0.0-rc.9",
        "gulp": "^3.9.1",
        "gulp-json5-to-json": "^0.2.1",
        "gulp-merge-json": "^1.3.0",
        "json5": "^2.2.3",
        "lodash": "^4.17.21"
    }
}
