{
    "name": "vscode-LANGUAGE-syntax",
    "displayName": "vscode-LANGUAGE-syntax",
    "description": "Syntax highlighting for LANGUAGE.",
    "version": "0.0.1",
    "publisher": "dunstontc",
    "repository": {
        "type": "git",
        "url": "https://github.com/dunstontc/vscode-LANGUAGE-syntax.git"
    },
    "bugs": {
        "url": "https://github.com/dunstontc/vscode-LANGUAGE-syntax/issues"
    },
    "scripts": {
        "start": "gulp watch"
    },
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "EXTENSION",
                "aliases": [
                    "EXTENSION"
                ],
                "extensions": [
                    ".EXTENSION"
                ],
                "filenames": [
                    "EXTENSIONfile"
                ],
                "firstLine": "^...",
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "EXTENSION",
                "scopeName": "source.EXTENSION",
                "path": "./syntaxes/EXTENSION.tmLanguage.json"
            }
        ]
    },
    "galleryBanner": {
        "color": "#1e1e1e",
        "theme": "dark"
    },
    "devDependencies": {
        "gulp": "^3.9.1",
        "gulp-json5-to-json": "^0.2.1",
        "gulp-merge-json": "^1.3.0"
    }
}
